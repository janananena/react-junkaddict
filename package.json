{
  "name": "junkaddict",
  "version": "0.1.0",
  "description": "a timetable for all the junk on streaming",
  "homepage": ".",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "dependencies": {
    "axios": "^1.9.0",
    "bootstrap": "^5.3.6",
    "json-server": "^1.0.0-beta.3",
    "lodash-es": "^4.17.21",
    "papaparse": "^5.5.3",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.0",
    "react-papaparse": "^4.4.0",
    "vite-plugin-commonjs": "^0.10.4",
    "web-vitals": "^5.0.1"
  },
  "scripts": {
    "vite:start": "concurrently --kill-others \"vite --host --port 3000\" \"json-server --watch src/data/dev-data-junk.json --host fatty --port 3004\"",
    "dev": "concurrently --kill-others \"vite --port 3000\" \"json-server --watch src/data/dev-data-junk.json --port 3004\"",
    "build": "tsc -b && vite build",
    "lint": "eslint . ",
    "lint-fix": "eslint . --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage --coverage.include=src",
    "docker-image": "docker build -t 'junkaddict/app:v0.1' -f build.Dockerfile .",
    "docker-run": "docker run -p 3000:3000 junkaddict/app:v0.1",
    "docker-server": "docker build -t 'junkaddict/server:v0.1' -f server.Dockerfile .",
    "docker-run-server": "docker run -p 3004:3004 junkaddict/server:v0.1",
    "check-dependencies": "npm-check-updates",
    "update-dependencies": "npm-check-updates -u"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash-es": "^4.17.12",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.27.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.1.0",
    "jsdom": "^26.1.0",
    "npm-check-updates": "^18.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": {
    "name": "jana",
    "email": "janananena@gmail.com"
  }
}
